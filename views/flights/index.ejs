<%- include('../partials/html-head') %> 
<%- include('../partials/nav') %> 

<main>
  <h1>Flight List</h1>
  <table>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight Number</th>
        <th>Departs</th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(flight => { %> 
        <tr>
          <td><%= flight.airline %></td>
          <td><%= flight.airport %></td>
          <td><%= flight.flightNo %></td>
          <td><%= flight.departs %></td>
        </tr>
      <% }) %> 
    </tbody>
  </table>
  <a href="/">Go Home</a>
</main>

<%- include('../partials/footer') %> 